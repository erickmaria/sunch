{
  "name": "sunch",
  "description": "search applition to do something",
  "author": {
    "name": "Erick Maria",
    "email": "erickmaria08@gmail.com"
  },
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "main": "dist-electron/main.js",
  "homepage": "./",
  "scripts": {
    "scraper-server": "node dist-scraper/app.js",
    "scraper-build": "tsc --build tsconfig.scraper.json",
    "scraper-dev": "tsc --build tsconfig.scraper.json && node dist-scraper/index.js",
    "dev": "vite && tsc ",
    "build": "tsc && vite build",
    "build:win": "NODE_ENV=local npm run build && electron-builder --win portable",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "electron ."
  },
  "build": {
    "appId": "com.emrds.sunch",
    "productName": "sunch",
    "files": [
      "dist/**/*",
      "dist-electron"
    ],
    "directories": {
      "buildResources": "public"
    }
  },
  "dependencies": {
    "@google/generative-ai": "^0.2.0",
    "@huggingface/agents": "^0.0.5",
    "@huggingface/inference": "^2.6.1",
    "@types/express": "^4.17.20",
    "body-parser": "^1.20.2",
    "express": "^4.18.2",
    "install": "^0.13.0",
    "lucide-react": "^0.277.0",
    "npm": "^10.1.0",
    "openai": "^4.25.0",
    "puppeteer": "^21.3.4",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-replace": "^5.0.2",
    "@types/body-parser": "^1.19.4",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.15",
    "electron": "^25.1.1",
    "electron-builder": "^24.9.1",
    "electron-is-dev": "^2.0.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "postcss": "^8.4.29",
    "tailwindcss": "^3.3.3",
    "tree-kill": "^1.2.2",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vite-plugin-commonjs-externals": "^0.1.3",
    "vite-plugin-electron": "^0.15.6"
  }
}
